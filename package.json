{
  "name": "trading-system",
  "version": "1.0.0",
  "description": "自动化交易系统",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "auth": "node src/app.js",
    "cal": "node src/cal.js",
    "plus": "node --inspect=127.0.0.1:9239 src/plus.js",
    "dev": "nodemon src/app.js",
    "test:header": "node scripts/test-push-header.js",
    "test:config": "node scripts/test-push-config.js",
    "test:account": "node scripts/test-account.js",
    "test:all": "node scripts/test-all.js",
    "build": "ncc build src/app.js -o dist -m ",
    "test:status": "curl -s http://localhost:3000/health | json_pp || curl -s http://localhost:3000/health",
    "debug": "node scripts/debug-http.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "express": "^4.18.2",
    "socket.io-client": "^4.7.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3",
    "nodemon": "^3.0.1"
  }
}